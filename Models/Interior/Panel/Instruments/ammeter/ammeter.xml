<?xml version="1.0"?>
  <!-- Ammeter -->
  <PropertyList> 
    <path>ammeter.ac</path>
    <animation> 
      <type>rotate</type>
      <object-name>needle</object-name>
      <property>systems/electrical/amps</property>
      <interpolation>
        <entry>
          <ind>-50</ind>
          <dep>-50</dep>
        </entry>
        <entry> 
          <ind>50</ind>
          <dep>50</dep>
        </entry>
      </interpolation>
      <center>
        <x-m>0.00229331</x-m>
        <y-m>0</y-m>
        <z-m>-0.01344</z-m>
      </center>
      <axis>
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
    <animation>
      <type>material</type>
      <emission>
        <factor-prop>controls/lighting/panel-norm</factor-prop>
        <red>0.65</red>
        <green>0.10</green>
        <blue>0.10</blue>
      </emission>
    </animation>
    <animation>
		<type>noshadow</type>
    </animation>
    <!-- *** LoD *** -->
    <animation> 
      <type>range</type>
      <min-m>0</min-m>
      <max-m>5</max-m>
    </animation>
  </PropertyList>
