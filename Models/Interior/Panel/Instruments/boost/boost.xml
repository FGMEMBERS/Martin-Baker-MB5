<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList> 

  <path>boost.ac</path>
    
  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <object-name>limiter</object-name>

    <object-name>vitre</object-name>
  </animation>

  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>vitre</object-name>
  </effect>
  
  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <object-name>limiter</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>

  <animation> 
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>engines/engine[0]/boost-gauge-inhg</property>
    <!-- <factor>0.4911</factor>
    <offset>-29.92</offset> -->
    <interpolation> 
      <entry><ind> -4 </ind><dep> -43 </dep></entry>
      <entry><ind> -2 </ind><dep> -22 </dep></entry>
      <entry><ind>  0 </ind><dep>   0 </dep></entry>
      <entry><ind>  2 </ind><dep>  21 </dep></entry>
      <entry><ind>  4 </ind><dep>  42 </dep></entry>
      <entry><ind>  6 </ind><dep>  63 </dep></entry>
      <entry><ind>  8 </ind><dep>  85 </dep></entry>
      <entry><ind> 10 </ind><dep> 106 </dep></entry>
      <entry><ind> 12 </ind><dep> 126 </dep></entry>
      <entry><ind> 14 </ind><dep> 147 </dep></entry>
      <entry><ind> 16 </ind><dep> 167 </dep></entry>
      <entry><ind> 18 </ind><dep> 189 </dep></entry>
      <entry><ind> 20 </ind><dep> 209 </dep></entry>
      <entry><ind> 26 </ind><dep> 272 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
    
</PropertyList>
